



export const logOut = async () => {
    try {
        const res = await fetch(`${process.env.NEXT_PUBLIC_API_URL}/auth/logout`, {
            method: "POST",
            credentials: "include"
        })

        if (!res.ok) {
            throw new Error("Failed to log out")
        }

        return { success: true }

    } catch (err) {
        console.log(err)
        return { success: false, error: err }
    }
}